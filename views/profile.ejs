<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% include application %>
    <div class="container">
      <div class="row">
        <div class="col-md-3">

        </div>

        <div class="col-md-9">
          <h1><%=user.first_name%> <%=user.last_name%></h1>
          <h3>
            <% if (user.job_title=="Entrepreneur") {%>
              <span class="job-title-icon"><i class="fa fa-lightbulb-o"></i></span>
            <%}else if (user.job_title=="Developer") {%>
              <span class="job-title-icon"><i class="fa fa-code"></i></span>
            <%}else if (user.job_title=="Investor") {%>
              <span class="job-title-icon"><i class="fa fa-dollar"></i></span>
            <%}%>
            <%=user.job_title%>
          </h3>
          <div class="well">
            <%=user.description%>
          </div>
          <%=user.phone_number%><br>
          <%=user.contact_email%><br>

        </div>
      </div>
    </div>


  </body>
</html>
